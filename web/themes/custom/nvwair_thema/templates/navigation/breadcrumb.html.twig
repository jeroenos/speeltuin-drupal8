{#
/**
 * @file
 * Theme override for a breadcrumb trail.
 *
 * Available variables:
 * - breadcrumb: Breadcrumb trail items.
 */
#}
{% if breadcrumb %}
  {% include directory ~ "/templates/navigation/breadcrumb.twig" with {
      modifier_class: '',
      breadcrumb: breadcrumb
    } only %}
{% endif %}
