<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:bte="http://www.minez.nl/xml/schema/oba/basistypes/1.0" xmlns:pub="http://www.minez.nl/xml/schema/oba/publicatie/v2" xmlns:mhdr="http://www.minez.nl/xml/schema/gen/header/1.0" xmlns:odl="http://www.minez.nl/xml/schema/oba/oordeel/1.0" targetNamespace="http://www.minez.nl/xml/schema/oba/publicatie/v2" elementFormDefault="qualified" version="1.0">
	<xs:import namespace="http://www.minez.nl/xml/schema/oba/basistypes/1.0" schemaLocation="obaBasisTypes.xsd"/>
	<!-- =========== Smple Types =========== -->
	<xs:simpleType name="besluitCodeType">
		<xs:restriction base="xs:string">
			<!--xs:enumeration value="GROEN"/>
			<xs:enumeration value="ORANJE"/>
			<xs:enumeration value="ROOD"/>
			<xs:enumeration value="GEEL"/>
			<xs:enumeration value="WIT"/-->
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="oordeelCodeType">
		<xs:restriction base="xs:string">
			<!--xs:enumeration value="OK"/>
			<xs:enumeration value="NOK"/>
			<xs:enumeration value="NVT"/>
			<xs:enumeration value="NB"/-->
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="publicatieDatum">
		<xs:restriction base="xs:date"/>
	</xs:simpleType>
	<!-- =========== Complex Types =========== -->
	<xs:complexType name="domeinType">
		<xs:sequence>
			<xs:element name="bedrijfstype" type="xs:string"/>
			<xs:element name="thema" type="xs:string"/>
			<xs:element name="omschrijving" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="headerType">
		<xs:sequence>
			<xs:element name="batchId" type="xs:integer"/>
			<xs:element name="totaal" type="xs:integer"/>
			<xs:element name="volgNr" type="xs:integer"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="maatregelType">
		<xs:sequence>
			<xs:element name="datum" type="xs:date"/>
			<xs:element name="status" type="xs:string"/>
			<xs:element name="soort" type="xs:string"/>
			<xs:element name="beslissing" type="xs:string"/>
			<xs:element name="motivering" type="xs:string" minOccurs="0"/>
			<xs:element name="reactie" type="xs:string" minOccurs="0"/>
			<xs:element name="bedrag" type="bte:decimalType"/>
			<xs:element name="overtredingen" type="pub:overtredingenType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="objectType">
		<xs:sequence>
			<xs:element name="handelsnaam" type="xs:string"/>
			<xs:element name="kvknummer" type="xs:string"/>
			<xs:element name="vestigingsnummer" type="xs:string"/>
			<xs:element name="adres" type="bte:adresType"/>
			<xs:element name="locatie" type="bte:locatieType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="onderwerpOordeelType">
		<xs:sequence>
			<xs:element name="onderwerp" type="xs:string"/>
			<xs:element name="oordeel" type="xs:string"/>
			<xs:element name="omschrijving" type="xs:string" minOccurs="0"/>
			<xs:element name="grondslag" type="xs:string"/>
			<xs:element name="datumogrondslag" type="xs:date"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="oordeelType">
		<xs:sequence>
			<xs:element name="domein" type="pub:domeinType"/>
			<xs:element name="oordeel" type="xs:string"/>
			<xs:element name="omschrijving" type="xs:string" minOccurs="0"/>
			<xs:element name="datumontstaan" type="xs:date"/>
			<xs:element name="reactie" type="xs:string" minOccurs="0"/>
			<xs:element name="onderwerpoordelen" type="pub:onderwerpOordelenType"/>
			<xs:element name="formuleoordeel" type="xs:string" minOccurs="0"/>
			<xs:element name="formulenaam" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="overtredingType">
		<xs:sequence>
			<xs:element name="feit" type="xs:string"/>
			<xs:element name="bedrag" type="bte:decimalType"/>
			<xs:element name="overtreding" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="publicatieType">
		<xs:sequence>
			<xs:element name="object" type="pub:objectType"/>
			<xs:element name="oordelen" type="pub:oordelenType"/>
			<xs:element name="maatregelen" type="pub:maatregelenType" minOccurs="0"/>
			<xs:element name="publicatiedatum" type="pub:publicatieDatum"/>
		</xs:sequence>
	</xs:complexType>
	<!-- =========== Complex Array Types =========== -->
	<xs:complexType name="maatregelenType">
		<xs:sequence>
			<xs:element name="maatregel" type="pub:maatregelType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="onderwerpOordelenType">
		<xs:sequence>
			<xs:element name="onderwerpoordeel" type="pub:onderwerpOordeelType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="oordelenType">
		<xs:sequence>
			<xs:element name="oordeel" type="pub:oordeelType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="overtredingenType">
		<xs:sequence>
			<xs:element name="overtreding" type="pub:overtredingType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="publicatiesType">
		<xs:sequence>
			<xs:element name="publicatie" type="pub:publicatieType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- =========== Elements =========== -->
	<xs:element name="drupalPublicatie">
		<xs:complexType>
			<xs:sequence>
				<!--xs:element ref="mhdr:messageHeader"/-->
				<xs:element name="header" type="pub:headerType"/>
				<xs:element name="drupal" type="pub:publicatiesType"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
