        "d3v4/d3": "4.9.1",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/charts": "^3.1",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.7.0",
        "drupal/easychart": "^3.4",
